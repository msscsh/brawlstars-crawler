{
    "name": "bs-club-mngr",
    "version": "1.0.0",
    "description": "A library that helps to gain more Club organization.",
    "main": "dist/index.js",
    "module": "dist/index.m.js",
    "unpkg": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "keys": "rm -rf shell.json cypress.env.json && sh src/commons/scripts/create-cypress-env.sh && npx cypress run --spec 'cypress/e2e/login.spec.cy.js' --headless ",
        "files": "bash src/commons/scripts/create-json-files.sh",
        "build": "rm -rf dist && tsc && copyfiles -u 1 src/**/*.txt src/**/*.json dist",
        "server": "npx nodemon src/server.js",
        "pipeline": "npm run keys && npm run files && npm run build && npm run server"
    },
    "author": "msscash",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/node": "^20.2.1",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "cypress": "^12.17.1",
        "eslint": "^8.46.0",
        "eslint-plugin-prettier": "^5.0.0",
        "node-fetch": "^3.3.1",
        "nodemon": "^2.0.7",
        "path-browserify": "^1.0.1",
        "prettier": "^3.0.0",
        "process": "^0.11.10",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "brawlstars-api-sdk": "^1.0.0",
        "copyfiles": "^2.4.1",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "googleapis": "^121.0.0",
        "serve-favicon": "^2.5.0"
    }
}
